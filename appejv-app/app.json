{
  "expo": {
    "name": "SLM Solar",
    "slug": "slmapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.slmsolar.app",
      "buildNumber": "1",
      "requireFullScreen": false,
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photos for uploading images.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to photos for saving images.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show nearby services.",
        "UIBackgroundModes": ["fetch", "remote-notification", "fetch", "remote-notification"]
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "scheme": "slmapp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "42c1a940-b7d6-4eb8-8321-f4d0713c415f"
      }
    },
    "android": {
      "package": "com.slmsolar.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "jsEngine": "hermes"
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/42c1a940-b7d6-4eb8-8321-f4d0713c415f"
    }
  }
}
